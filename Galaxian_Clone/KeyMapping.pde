class KeyMapping {
  
  int leftKey, upKey, rightKey, downKey;
  int aKey, bKey, selectKey, startKey;
  
  KeyMapping(int _left, int _up, int _right, int _down, int _a, int _b, int _select, int _start) {
    leftKey = _left;
    upKey = _up;
    rightKey = _right;
    downKey = _down;
    aKey = _a;
    bKey = _b;
    selectKey = _select;
    startKey = _start;
  }
  
}
